{
    "name": "Newsline",
    "description": "Read RSS feeds with news directly in browser.",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "16":"icons/16.png",
        "32":"icons/32.png",
        "48":"icons/48.png",
        "128":"icons/128.png"
    },
    "action": {
        "default_popup":"popup.html"
    },
    "permissions": ["scripting", "activeTab", "tabs", "http://127.0.0.1:8000/"],
    "background":{}
}